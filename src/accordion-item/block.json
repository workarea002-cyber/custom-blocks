{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "custom-blocks/accordion-item",
	"version": "0.1.0",
	"title": "Accordion Item",
	"parent": ["custom-blocks/accordion"],
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"headingTextAlign": {
			"type": "string"
		},
		"headingContent": {
			"type": "rich-text",
			"source": "rich-text",
			"selector": "h1,h2,h3,h4,h5,h6"
		},
		"headingTag": {
			"type": "string",
			"default": "h3"
		}
	},
	"textdomain": "my-custom-blocks",
	"editorScript": "file:./index.js"
}

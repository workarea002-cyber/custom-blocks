{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "custom-blocks/accordion",
	"version": "0.1.0",
	"title": "Accordion",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"attributes": {
		"textColor": {
			"type": "string",
			"default": ""
		},
		"backgroundColor": {
			"type": "string",
			"default": ""
		},
		"paddingTop": {
			"type": "string",
			"default": "1rem"
		},
		"paddingRight": {
			"type": "string",
			"default": "1rem"
		},
		"paddingBottom": {
			"type": "string",
			"default": "1rem"
		},
		"paddingLeft": {
			"type": "string",
			"default": "1rem"
		},
		"gap": {
			"type": "string",
			"default": "1rem"
		}
	},
	"supports": {
		"html": false,
		"border": {
			"color": true,
			"radius": true,
			"width": true,
			"style": true
		},
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"blockGap": true,
			"__experimentalDefaultControls": {
				"margin": false,
				"padding": false,
				"blockGap": true
			}
		},
		"__experimentalLayout": {
			"allowSwitching": false,
			"default": {
				"type": "flex",
				"orientation": "vertical"
			}
		},
		"__experimentalBorder": {
			"color": true,
			"radius": true,
			"width": true,
			"style": true
		},
		"allowedBlocks": true
	},
	"textdomain": "my-custom-blocks",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
